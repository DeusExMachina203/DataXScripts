#query 1
#que negociadores tienen tanto correo como telefono
{
  filter: {
    Correos: {
      $exists: true,
      $ne: []
    },
    Telefono: {
      $ne: null
    }
  }
}

#query 2
#saber que pedidos de cierta empresa tienen un costo total mayor a 1000 soles
{ 
  "IdEmpresa": 20, 
  "CostoTotal": { "$gt": 1000 } 
}
