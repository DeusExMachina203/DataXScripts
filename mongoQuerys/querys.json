{
  "collation": null,
  "filter": {
    "FechaInicio": {
      "$gte": "2023-12-31"
    },
    "FechaFinal": {
      "$lte": "2024-12-31"
    }
  },
  "limit": null,
  "project": null,
  "skip": null,
  "sort": null
}
